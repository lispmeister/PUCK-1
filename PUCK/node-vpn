
var puckid = "9FD81FE0F99E590BF56D98AC6E8467A099F22521"
var ipaddr = "192.168.0.138"

vpn   = '/etc/puck/exe/start_vpn.sh'

args = [puckid, ipaddr]
cmd = vpn

var fs = require('fs'),

spawn = require('child_process').spawn,

out = fs.openSync('tmp/out.log', 'a'),

err = fs.openSync('tmp/out.log', 'a');

var child = spawn(cmd, args, {
   detached: true,
   stdio: [ 'ignore', out, err ]
});

child.unref();

